{
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "gsap": "readonly",
    "ScrollTrigger": "readonly",
    "mixpanel": "readonly",
    "Termly": "readonly",
    "ire": "readonly",
    "Intercom": "readonly",
    "Swiper": "readonly",
    "Cookies": "readonly"
  },
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-unused-vars": "warn",
    "no-console": "warn",
    "no-undef": "error"
  }
}
